<% if(errors.length>0){%> 
	<div class="row">
		<div class="col my-3">
			<div class="alert alert-danger">
				<%errors.forEach(e=>{%>
				<%=errors%><br>
				<%});%>
			</div>
		</div>
	</div>
<%} %>

<% if(succes.length>0){%> 
	<div class="row">
		<div class="col my-3">
			<div class="alert alert-success">
				<%succes.forEach(succes=>{%>
				<%=succes%><br>
				<%});%>
			</div>
		</div>
	</div>
<%} %>